<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/background"
    android:id="@+id/relative"
    tools:context=".Fragment2">

    <TextView
        android:id="@+id/objective"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:fontFamily="@font/alegreya_sc"
        android:paddingStart="10sp"
        android:paddingTop="100sp"
        android:paddingBottom="10sp"
        android:text="Objective"
        android:textAppearance="@style/Widget.AppCompat.Spinner.Underlined"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,RtlSymmetry" />

    <View
        android:id="@+id/line2"
        android:layout_width="200sp"
        android:layout_height="1sp"
        android:layout_below="@id/objective"
        android:layout_alignParentStart="true"
        android:layout_centerInParent="true"
        android:background="@android:color/black"
        tools:ignore="SmallSp" />


    <TextView
        android:id="@+id/t6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/line2"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textMultiLine"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Objective"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/employment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t6"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:fontFamily="@font/alegreya_sc"
        android:inputType="textPersonName"
        android:paddingStart="10sp"
        android:paddingTop="20sp"
        android:paddingBottom="10sp"
        android:text="Employment"
        android:textAppearance="@style/Widget.AppCompat.Spinner.Underlined"
        android:textSize="16sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <View
        android:id="@+id/line3"
        android:layout_width="200sp"
        android:layout_height="1sp"
        android:layout_below="@id/employment"
        android:layout_alignParentStart="true"
        android:layout_centerInParent="true"
        android:background="@android:color/black"
        tools:ignore="SmallSp" />

    <TextView
        android:id="@+id/t20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/line3"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textPostalAddress"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Employer"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/t8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t20"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textMultiLine"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Job Description"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/t9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t8"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textMultiLine"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Skills"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/t10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t9"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textMultiLine"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Experience"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />


    <TextView
        android:id="@+id/education"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t10"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:fontFamily="@font/alegreya_sc"
        android:inputType="textPersonName"
        android:paddingStart="10sp"
        android:paddingTop="20sp"
        android:paddingBottom="10sp"
        android:text="Education"
        android:textAppearance="@style/Widget.AppCompat.Spinner.Underlined"
        android:textSize="16sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <View
        android:id="@+id/line4"
        android:layout_width="200sp"
        android:layout_height="1sp"
        android:layout_below="@id/education"
        android:layout_alignParentStart="true"
        android:layout_centerInParent="true"
        android:background="@android:color/black"
        tools:ignore="SmallSp" />

    <TextView
        android:id="@+id/t11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/line4"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="text"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="College name"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <LinearLayout
        android:id="@+id/lay2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@id/t11">

        <TextView
            android:id="@+id/t12"
            android:layout_width="100sp"
            android:layout_height="match_parent"
            android:autoLink="all"
            android:inputType="text"
            android:paddingStart="5sp"
            android:paddingTop="5sp"
            android:text="Degree"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="visible"
            tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/t21"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:autoLink="all"
            android:inputType="number"
            android:paddingStart="5sp"
            android:paddingTop="5sp"
            android:text="Year of Passing"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="visible"
            tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
            tools:visibility="visible" />
    </LinearLayout>


    <TextView
        android:id="@+id/t13"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_below="@id/lay2"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="number"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="GPA"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />


    <TextView
        android:id="@+id/t14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t13"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textMultiLine"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Awards"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/t15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t14"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textMultiLine"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Certifications"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/t16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t15"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:inputType="textMultiLine"
        android:paddingStart="5sp"
        android:paddingTop="5sp"
        android:text="Hobbies"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,RtlSymmetry,TextViewEdits"
        tools:visibility="visible" />

</RelativeLayout>